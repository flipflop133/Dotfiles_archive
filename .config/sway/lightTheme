# colors
# define colors
set $bg	#f2f2f2
set $fg	#000000
set $black	#000000
set $sel_bg_color	#1a73e8
set $sel_fg_color	#ffffff
set $red	#d93025

# set colors
# class                 border  backgr. text    indicator child_border
client.focused          $fg     $sel_bg_color     $sel_fg_color  $red  $sel_bg_color
client.focused_inactive $fg     $sel_bg_color     $sel_fg_color $red  $sel_fg_color
client.unfocused        $fg     $bg     $black  $fg       $bg
client.urgent           $fg     $bg     $red    $fg       $fg
client.placeholder      $black  $lightBlack $fg     $black   $lightBlack 
client.background       $fg
